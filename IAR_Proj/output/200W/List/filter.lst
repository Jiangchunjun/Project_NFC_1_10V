     19          /* Macro Defines --------------------------------------------------------------------------------*/
     20          #define FILTER_RANK     10
     21          #define FILTER_RATIO    {0.2, 0.15, 0.15, 0.1, 0.1, 0.1, 0.05, 0.05, 0.05, 0.05}
     22          
     23          /* Private Variable -----------------------------------------------------------------------------*/
     24          const float  filter_ratio[FILTER_RANK] = FILTER_RATIO;
     25          
     26          struct
     27          {
     28              bool init_state;
     29              uint16_t output;
     30              uint16_t input[FILTER_RANK];
     31          } filter = 
