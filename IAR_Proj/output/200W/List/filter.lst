     44          /*******************************************************************************
     45          * @Brief   Filter Algorithm Init
     46          * @Param   
     47          * @Note    
     48          * @Return  
     49          *******************************************************************************/
     50          void Filter_Init(void)
     51          {
     52              uint16_t i = 0;
     53              
     54              for(i=0; i<FILTER_RANK; i++)
     55              {
     56                  filter.input[i] = 0;
     57              }
     58          
     59              filter.output = 0;
     60              filter.init_state = true;
     61          }
     62          
     63          
     64          /*******************************************************************************
     65          * @Brief   Filter Data Input
     66          * @Param   [in]din: input data
     67          * @Note    
     68          * @Return  filter result
     69          *******************************************************************************/
     70          uint16_t Filter_Input(uint16_t din)
     71          {
     72              uint16_t i = 0;
     73              float temp = 0;
     74          
     75              if(filter.init_state == true)
     76              {
     77                  /* first enter filter after init */
     78                  for(i=0; i<FILTER_RANK; i++)
     79                  {
     80                      filter.input[i] = din;
     81                  }
     82                  
     83                  filter.init_state = false;
     84                  filter.output = din;        
     85              }
     86              else
     87              {
     88                  /* shift data queue */
     89                  for(i=1; i<FILTER_RANK; i++)
     90                  {
     91                      filter.input[i] = filter.input[i-1];
     92                  }
     93                  filter.input[0] = din;
     94                  
     95                  /* filter with rank ratio */
     96                  for(i=0; i<FILTER_RANK; i++)
     97                  {
     98                      temp += filter.input[i] * filter_ratio[i];
     99                  }
    100                  
    101                  filter.output = (uint16_t)temp;
    102              }
    103              
    104              return filter.output;   
    105          }
    106          
