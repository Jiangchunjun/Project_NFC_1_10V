     47              
     48              /* Config watchdog boundary and start timer */
     49              XMC_WDT_Init(&wdt_config);
     50              XMC_WDT_SetMode(XMC_WDT_MODE_TIMEOUT);
     51              XMC_WDT_Start();
     52          }
